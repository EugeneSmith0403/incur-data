{
  "name": "@incur-data/indexer",
  "version": "1.0.0",
  "private": true,
  "description": "Indexer for discovering DLN transactions on Solana",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:src": "INDEXER_PORT=8081 DLN_PROGRAM_ID=src5qyZHqTqecJV4aY6Cb6zDZLMDzrDKKezs22MPHr4 tsx watch src/index.ts",
    "dev:dst": "INDEXER_PORT=8082 DLN_PROGRAM_ID=dst5MGcFPoBeREFAA5E3tU5ij8m5uVYwkzkSAbsLbNo tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:src": "INDEXER_PORT=8081 DLN_PROGRAM_ID=src5qyZHqTqecJV4aY6Cb6zDZLMDzrDKKezs22MPHr4 node dist/index.js",
    "start:dst": "INDEXER_PORT=8082 DLN_PROGRAM_ID=dst5MGcFPoBeREFAA5E3tU5ij8m5uVYwkzkSAbsLbNo node dist/index.js",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "test:websocket": "tsx src/test-websocket.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.5",
    "@incur-data/dtos": "workspace:*",
    "@incur-data/rabbitmq": "workspace:*",
    "@solana/web3.js": "^1.87.6",
    "amqplib": "^0.10.3",
    "redis": "^4.6.12",
    "fastify": "^4.25.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/amqplib": "^0.10.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0"
  }
}
